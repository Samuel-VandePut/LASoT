{
  "name": "lasot",
  "displayName": "lasot",
  "description": "Learn to assert in software testing",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.68.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:mutationTesting"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "lasot.DeactivateHighlights",
        "title": "Highlights Hints",
        "icon": "$(stop)"
      },
      {
        "command": "lasot.highlightsHints",
        "title": "Highlights Hints",
        "icon": "$(play)"
      },
      {
        "command": "executeGoal",
        "title": "execute goal",
        "icon": "$(play)"
      },
      {
        "command": "executeMultiStepGoal",
        "title": "execute goal"
      },
      {
        "command": "lasot.wizard",
        "title": "LASoT Wizard"
      },
      {
        "command": "mutationTesting.refresh",
        "title": "Refresh",
        "icon": "$(refresh)"
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "mutationTesting",
          "name": "Mutation Testing"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "mutationTesting.refresh",
          "when": "view == mutationTesting",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "executeGoal",
          "when": "view == mutationTesting && viewItem == goal",
          "group": "inline"
        },
        {
          "command": "lasot.DeactivateHighlights",
          "when": "view == mutationTesting && viewItem == lasot",
          "group": "inline"
        },
        {
          "command": "lasot.highlightsHints",
          "when": "view == mutationTesting && viewItem == lasot",
          "group": "inline"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mkdirp": "^1.0.2",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/rimraf": "^3.0.2",
    "@types/vscode": "^1.68.0",
    "@types/xml2js": "^0.4.11",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.16.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.2"
  },
  "dependencies": {
    "mkdirp": "^1.0.4",
    "rimraf": "^3.0.2",
    "xml2js": "^0.4.23"
  }
}
